NODE_ENV=development
API_PORT=3001

# Database
DATABASE_URL=postgresql://complianceos:complianceos_dev@localhost:5435/complianceos

# Redis
REDIS_HOST=localhost
REDIS_PORT=6380

# JWT
JWT_SECRET=your-super-secret-jwt-key-change-this-in-production
JWT_EXPIRES_IN=15m
JWT_REFRESH_SECRET=your-super-secret-refresh-key-change-this-in-production
JWT_REFRESH_EXPIRES_IN=7d

# S3
S3_ENDPOINT=http://localhost:9000
S3_ACCESS_KEY=minioadmin
S3_SECRET_KEY=minioadmin
S3_BUCKET_ARTIFACTS=evidence-artifacts
S3_BUCKET_PACKS=inspection-packs
S3_REGION=us-east-1
S3_USE_SSL=false

# Google OAuth
GOOGLE_CLIENT_ID=your-google-client-id
GOOGLE_CLIENT_SECRET=your-google-client-secret
GOOGLE_REDIRECT_URI=http://localhost:3001/api/integrations/google/callback

# File Upload
MAX_FILE_SIZE=52428800

# Rate Limiting
RATE_LIMIT_TTL=60
RATE_LIMIT_MAX=100
